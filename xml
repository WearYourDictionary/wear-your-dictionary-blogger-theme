   <script type='text/javascript'>
//<![CDATA[
function fetchRandomPost() {
    const feedUrl = "/feeds/posts/default?alt=json-in-script&max-results=100";  // Fetch up to 100 posts

    $.ajax({
        url: feedUrl,
        dataType: "jsonp",
        success: function(data) {
            const posts = data.feed.entry;  // Get the list of posts

            if (posts && posts.length > 0) {
                const randomIndex = Math.floor(Math.random() * posts.length);  // Pick a random post
                const randomPost = posts[randomIndex];

                // Extract title, link, and content
                const postTitle = randomPost.title.$t;
                const postUrl = randomPost.link.find(link => link.rel === 'alternate').href;
                const postContent = randomPost.content ? randomPost.content.$t : "No content available";

                // Display the random post on the homepage
                document.querySelector('.random-post').innerHTML = `
                    <h2><a href="${postUrl}" target="_blank">${postTitle}</a></h2>
                    <div>${postContent}</div>
                `;
            } else {
                document.querySelector('.random-post').innerHTML = "<p>No posts available.</p>";
            }
        },
        error: function(jqXHR, textStatus, errorThrown) {
            console.error("Error fetching posts:", textStatus, errorThrown);
            document.querySelector('.random-post').innerHTML = "<p>Error fetching posts.</p>";
        }
    });
}

// Fetch a random post if the page is the homepage
if (data:blog.pageType == 'index') {
    fetchRandomPost();
}
//]]>
</script>

      </b:if>  
       <b:if cond='data:blog.pageType != &quot;index&quot;'>
